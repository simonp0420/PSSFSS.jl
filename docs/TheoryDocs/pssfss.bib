@INPROCEEDINGS{scmo:06,
  title = {A New Low Loss Rotman Lens Design for Multibeam Phased Arrays},
  author = {Schulwitz, L. and Mortazawi, A.},
  booktitle = {Microwave Symposium Digest, 2006. {IEEE} {MTT}-S International},
  year = {2006},
  pages = {445--448},
  address = {San Francisco, CA},
  month = jun,
  abstract = {The design methodology and analysis of a new low loss Rotman lens
       beam-forming network is presented. With the introduction of
       dielectric contours of varying permittivity within the lens,
       enhanced focusing is achieved. The varying permittivity is
       realized through a synthesized dielectric technique, where a
       periodic lattice of holes is formed within the dielectric
       substrate. In comparison to a conventional Rotman lens of
       similar shape and size, this new enhanced focused Rotman lens
       shows an insertion loss improvement of 1.2 to 2.0 dB. Also, the
       decrease in gain at the extreme scan angles is reduced from 1.5
       dB to 0.7 dB.},
  doi = {10.1109/MWSYM.2006.249587},
  ISSN = {0149-645X},
}

@inproceedings{shch:92,
   title = "A technique for the efficient computation of the periodic {G}reen's
     function in layered dielectric media",
   author = "R. M. Shubair and Y. L. Chow",
   booktitle = "IEEE MTT-S International Microwave Symposium Digest",
   volume = "1",
   year =    "1992",
   address = "Albuquerque, NM",
   month = jun,
   kwds = "microstrip Green's function periodic sommerfeld integral DCIT efficient analysis",
   pages = "393--396",
   annote = "The authors present a novel technique for the efficient
computation of the periodic Green's function in layered dielectric media.
This technique is based on approximating the spectral Green's function by a
set of inverse-transformable complex exponential functions. This enables
the use of Poisson's summation formula to express the periodic Green's
function as a combination of a spectral series and a spatial series each of
which is rapidly convergent. The proposed technique can be applied to a
wide class of problems where periodic structures are to be modeled."
}

