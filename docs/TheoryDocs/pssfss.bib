@ARTICLE{mabm:08,
  title = {Reduction of Truncation Errors in Planar Near-Field
           Aperture Antenna Measurements Using the {G}erchberg-{P}apoulis Algorithm},
  author = {Martini, E. and Breinbjerg, O. and Maci, S.},
   journal = ieeetaps,
  year = {2008},
  volume = {56},
  number = {11},
  pages = {3485--3493},
  month = nov,
  abstract = {A simple and effective procedure for the reduction of truncation
       errors in planar near-field measurements of aperture antennas
       is presented. The procedure relies on the consideration that,
       due to the scan plane truncation, the calculated plane wave
       spectrum of the field radiated by the antenna is reliable only
       within a certain portion of the visible region. Accordingly,
       the truncation error is reduced by extrapolating the remaining
       portion of the visible region by the Gerchberg-Papoulis
       iterative algorithm, exploiting a condition of spatial
       concentration of the fields on the antenna aperture plane. The
       proposed procedure is simple and computationally efficient; it
       does not require any modification of the measurement procedure
       and it allows for the usual probe correction. Far-field
       patterns reconstructed from both simulated and measured
       truncated near-field data demonstrate its effectiveness and
       stability against measurement inaccuracies.},
  keywords = {antenna radiation patterns, aperture antennas, iterative methods},
  doi = {10.1109/TAP.2008.2005442},
